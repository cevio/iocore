{
  "name": "@iocore/cache-dispenser-file",
  "version": "1.0.1",
  "description": "Default Monorepo template",
  "main": "dist/index.js",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "fix": "fix-esm-import-path --preserve-import-type ./dist",

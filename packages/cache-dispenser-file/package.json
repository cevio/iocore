  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "fix-esm-import-path": "^1.10.1"
  },
  "dependencies": {
    "@iocore/cache": "workspace:^",
    "@iocore/component": "workspace:^",
    "@types/fs-extra": "^11.0.4",
    "fs-extra": "^11.2.0",
    "glob": "^11.0.1"
  }
}
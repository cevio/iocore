{
  "name": "@iocore/cli",
  "version": "1.0.23",
  "description": "Default Monorepo template",
  "main": "dist/index.js",
  "type": "module",
  "files": [
    "dist",
    "template"
  ],
  "bin": {
    "iocore": "dist/index.js"
  },
  "scripts": {
    "fix": "fix-esm-import-path --preserve-import-type ./dist",
    "build": "rm -rf ./dist && mkdir ./dist && tsc -d && npm run fix"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "fix-esm-import-path": "^1.10.1"
  },
  "dependencies": {
    "@iocore/boot": "workspace:^",
    "@iocore/micro-ws-registry": "workspace:^",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "commander": "^13.1.0",
    "fs-extra": "^11.2.0",
    "inquirer": "^12.5.0",
    "ts-morph": "^25.0.1",
    "tsx": "^4.19.3",
    "typescript": "^5.6.3"
  }
}
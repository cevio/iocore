{
  "name": "@iocore/cli",
  "version": "1.0.1",
  "description": "Default Monorepo template",
  "main": "dist/index.js",
  "type": "module",
  "files": [
    "dist"
  ],
  "bin": {
    "iocore": "dist/index.js"
  },
  "scripts": {
    "fix": "fix-esm-import-path --preserve-import-type ./dist",
    "build": "rm -rf ./dist && mkdir ./dist && tsc -d && npm run fix"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "fix-esm-import-path": "^1.10.1"
  },
  "dependencies": {
    "@iocore/boot": "workspace:^",
    "commander": "^13.1.0",
    "tsx": "^4.19.3"
  }
}